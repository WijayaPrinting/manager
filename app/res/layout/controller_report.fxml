<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.VBox?>
<TableView xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml"
           fx:controller="com.hendraanggrian.openpss.ui.report.ReportController"
           fx:id="table" VBox.vgrow="ALWAYS">
    <columns>
        <TableColumn fx:id="dateColumn" text="%date"/>
        <TableColumn fx:id="cashColumn" text="%cash"/>
        <TableColumn fx:id="transferColumn" text="%transfer"/>
        <TableColumn fx:id="totalColumn" text="%total"/>
    </columns>
    <columnResizePolicy>
        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
    </columnResizePolicy>
</TableView>