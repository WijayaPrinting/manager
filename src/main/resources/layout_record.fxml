<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="640.0"
      prefWidth="768.0" spacing="8.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="com.wijayaprinting.javafx.controller.RecordController">
    <children>
        <HBox alignment="CENTER_LEFT">
            <VBox.margin>
                <Insets left="8.0" right="8.0" top="8.0"/>
            </VBox.margin>
            <children>
                <ButtonBar buttonMinWidth="192.0" HBox.hgrow="ALWAYS">
                    <buttons>
                        <Button cancelButton="true" mnemonicParsing="false" onAction="#buttonCancelOnAction"
                                text="%cancel"/>
                        <Button fx:id="buttonPrint" defaultButton="true" mnemonicParsing="false"
                                onAction="#buttonPrintOnAction" text="%print"/>
                    </buttons>
                    <HBox.margin>
                        <Insets left="8.0"/>
                    </HBox.margin>
                </ButtonBar>
            </children>
        </HBox>
        <TreeTableView fx:id="treeTableView" editable="true" style="-fx-font-family: Tahoma;" VBox.vgrow="ALWAYS">
            <columns>
                <TreeTableColumn fx:id="treeTableColumnEmployee" editable="false" text="%employee"/>
                <TreeTableColumn fx:id="treeTableColumnStart" editable="false" maxWidth="156.0" minWidth="156.0"
                                 prefWidth="156.0" resizable="false" style="-fx-alignment: CENTER;" text="%start"/>
                <TreeTableColumn fx:id="treeTableColumnEnd" editable="false" maxWidth="156.0" minWidth="156.0"
                                 prefWidth="156.0" resizable="false" style="-fx-alignment: CENTER;" text="%end"/>
                <TreeTableColumn fx:id="treeTableColumnDaily" maxWidth="64.0" minWidth="64.0" prefWidth="64.0"
                                 resizable="false" style="-fx-alignment: CENTER;" text="%daily"/>
                <TreeTableColumn fx:id="treeTableColumnDailyIncome" editable="false"
                                 style="-fx-alignment: CENTER-RIGHT;" text="%daily_income"/>
                <TreeTableColumn fx:id="treeTableColumnOvertime" maxWidth="64.0" minWidth="64.0" prefWidth="64.0"
                                 resizable="false" style="-fx-alignment: CENTER;" text="%overtime"/>
                <TreeTableColumn fx:id="treeTableColumnOvertimeIncome" editable="false"
                                 style="-fx-alignment: CENTER-RIGHT;" text="%overtime_income"/>
                <TreeTableColumn fx:id="treeTableColumnTotal" editable="false" style="-fx-alignment: CENTER-RIGHT;"
                                 text="%total"/>
            </columns>
            <columnResizePolicy>
                <TreeTableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
            </columnResizePolicy>
        </TreeTableView>
    </children>
</VBox>
