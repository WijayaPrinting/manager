<?xml version="1.0" encoding="UTF-8"?>

<!-- AnchorPane is necessary for displaying loadingPane in the center. -->
<?import javafx.scene.layout.*?>
<?import ktfx.jfoenix._JFXScrollPane?>
<AnchorPane xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.hendraanggrian.openpss.ui.wage.WageController"
            fx:id="anchorPane" VBox.vgrow="ALWAYS">
    <_JFXScrollPane fx:id="scrollPane" AnchorPane.leftAnchor="0" AnchorPane.bottomAnchor="0"
                    AnchorPane.rightAnchor="0" AnchorPane.topAnchor="0">
        <FlowPane fx:id="flowPane"/>
    </_JFXScrollPane>
</AnchorPane>